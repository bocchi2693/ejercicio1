class Libro:
    def __init__(self, titulo, autor, paginas):
        self.titulo = titulo
        self.autor = autor
        self.paginas = paginas
    def __str__(self):
        return f"'{self.titulo}' de {self.autor}"
    def __len__(self):
        return self.paginas

libro1 = Libro("Cien años de soledad", "Gabriel García Márquez", 471)
print(libro1)
print(len(libro1))
